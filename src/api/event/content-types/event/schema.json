{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "EventName": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "richtext",
      "required": true
    },
    "Address": {
      "type": "string",
      "required": true
    },
    "Island": {
      "type": "enumeration",
      "enum": [
        "Kauaʻi",
        "Oʻahu",
        "Molokaʻi",
        "Lānaʻi",
        "Maui",
        "Hawaiʻi (The Big Island)"
      ]
    },
    "StartDateTime": {
      "type": "datetime"
    },
    "EndDateTime": {
      "type": "datetime"
    },
    "Lng": {
      "type": "float"
    },
    "Lat": {
      "type": "float"
    },
    "Phone": {
      "type": "string"
    },
    "RepeatedEvent": {
      "type": "boolean"
    },
    "Day": {
      "type": "enumeration",
      "enum": [
        "Monday",
        "Tuesday",
        "Wednesday",
        "Thursday",
        "Friday",
        "Saturday",
        "Sunday"
      ]
    },
    "PointOfContact": {
      "type": "string"
    },
    "CoverPicture": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
